Option Explicit
'On Error Resume Next

Dim fso, objShell, maxsignattempts, timeoutbetweenattempts, signtoolpath, oFilesToEncode, i, file, oFile, iterations, command, popup, status, ObjExec, strFromProc

maxsignattempts = 2
timeoutbetweenattempts = 2

Set fso = CreateObject("Scripting.FileSystemObject")
Set objShell = WScript.CreateObject("WScript.Shell")

If fso.FileExists("C:\Program Files (x86)\Windows Kits\10\App Certification Kit\signtool.exe") Then
	signtoolpath = "C:\Program Files (x86)\Windows Kits\10\App Certification Kit\signtool.exe"
ElseIf fso.FileExists("C:\Program Files (x86)\Windows Kits\10\bin\10.0.18362.0\x64\signtool.exe") Then
	signtoolpath = "C:\Program Files (x86)\Windows Kits\10\bin\10.0.18362.0\x64\signtool.exe"
Else
	WScript.Echo "Cannot find signtool.exe."
End If

Set oFilesToEncode = WScript.Arguments

'WScript.Echo oFilesToEncode.Count

If oFilesToEncode.Count = 0 Then
	WScript.Echo "You must specify at least one script/executable to sign."
	WScript.Quit
End If

For i = 0 to oFilesToEncode.Count - 1
    file = oFilesToEncode(i)
    Set oFile = fso.GetFile(file)
	'WScript.Echo file
	fso.CopyFile file, oFile.ParentFolder & "\before_" & oFile.Name & "_before"
	iterations = 0
	status = 1
	While status > 0 AND iterations < maxsignattempts
		command = """" & signtoolpath & """ sign /tr http://timestamp.cyopki.com/OF5QFrRNze /td sha256 /fd sha256 /a """ & file & """"
		'WScript.Echo command
		'status = objShell.Run(command, ,True)
		strFromProc = ""
		Set ObjExec = objShell.Exec(command)
		Do
			If strFromProc = "" Then
				strFromProc = ObjExec.StdErr.ReadLine()
			Else
				strFromProc = strFromProc & vbCrlf & ObjExec.StdErr.ReadLine()
			End If
			'WScript.Echo "Output is: " & strFromProc
		Loop While Not ObjExec.StdErr.atEndOfStream
		status = ObjExec.ExitCode
		If status > 0 Then
			iterations = iterations + 1
			popup = CreateObject("WScript.Shell").Popup("Signing failed with the error below. Trying again in " & timeoutbetweenattempts & " seconds..." & vbCrlf & vbCrlf & vbTab & strFromProc, timeoutbetweenattempts, "WARNING", 1)
			If popup = 2 Then
				WScript.Quit
			End If
		End If
	Wend
	If status > 0 Then
		CreateObject("WScript.Shell").Popup "The below command failed after " & maxsignattempts & " attempts..."& vbCrlf & vbCrlf & vbTab & strFromProc, 0, "ERROR"
	End If
Next
